/ {
	hid_dev_0: hid_dev_0 {
		compatible = "zephyr,hid-device";
		interface-name = "HID0";
		boot-protocol-code = "keyboard";
		polling-rate = <1>;
		in-report-size = <64>;
		out-report-size = <64>;
	};
};
